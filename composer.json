{
	"name": "ran/my-plugin",
	"description": "A WordPress plugin biolerplate.",
	"type": "wordpress-plugin",
	"license": "MIT",
	"authors": [
		{
			"name": "Benjmn Rush <bnjmnrsh>",
			"email": "bnjmnrsh@gmail.com"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"autoload": {
		"psr-4": {
			"Ran\\MyPlugin\\": "inc/",
			"Ran\\PluginLib\\": "vendor/ran/plugin-lib/inc/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"repositories": [
		{
			"url": "https://github.com/RocketsAreNostalgic/PluginLib.git",
			"type": "git"
		}
	],
	"require": {
		"ran/plugin-lib": "*"
	},
	"require-dev": {
		"php": ">=8.0",
		"squizlabs/php_codesniffer": "*",
		"wp-coding-standards/wpcs": "*",
		"dealerdirect/phpcodesniffer-composer-installer": "*",
		"yoast/phpunit-polyfills": "^1.0",
		"phpunit/phpunit": "^9"
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"scripts": {
		"check-cs": [
			"@php ./vendor/bin/phpcs"
		],
		"fix-cs": [
			"@php ./vendor/bin/phpcbf"
		]
	},
	"support": {
		"issues": "https://github.com/RocketsAreNostalgic/PluginStarter/issues",
		"source": "https://github.com/RocketsAreNostalgic/PluginStarter"
	}
}
